

# Enable/disable strategies (comma-separated)
ENABLED_STRATEGIES=ORB,MARUBOZU,ENGULFING,INSIDEBAR,GAPREV

# Mode
DRY_RUN=false   # set to false for live trading

# Lot size & strikes
QUANTITY=50          # fallback lot size if not fetched from option chain
STRIKE_INTERVAL=50   # NIFTY = 50, BANKNIFTY = 100
OTM_STEPS=0          # 0 = ATM, 1 = 1 step OTM, etc.

# Candle config
CANDLE_INTERVAL=5     # 5-min candles
CANDLE_START=09:15    # Market start time
CANDLE_END=15:15      # Market close time

# ORB (Opening Range Breakout) window
ORB_START=09:15
ORB_END=09:30

# Safety exit (square-off all positions)
SAFETY_EXIT=15:15

# Risk management
MAX_DAILY_LOSS=5000       # max allowed loss for the day
PREMIUM_SL_PCT=50         # stoploss (premium drop %)
PREMIUM_TP_PCT=100        # target (premium gain %)

# Underlying index
UNDERLYING=NIFTY   # change to BANKNIFTY if needed

# Logging
LOG_FILE=orb_multi_strategy.log